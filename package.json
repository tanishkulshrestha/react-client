{
  "name": "training",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "commitizen": "^3.0.7",
    "prop-types": "^15.7.2",
    "proptypes": "^1.1.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-scripts": "2.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext jsx",
    "lint:fix": "eslint src --ext jsx --fix",
    "commit": "npx git-cz",
    "test:c": "test --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test:c",
      "pre-push": "npm run lint && npm run test:c"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "husky": "^1.3.1"
  }
}
